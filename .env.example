# .env.example

# Supabase (backend)
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# JWT for Express
JWT_SECRET=your_jwt_secret_here

# Port for backend (optional — defaults to 8000)
PORT=8000

# React frontend
# Point this at your backend’s public URL, e.g. https://api.yoursite.com
REACT_APP_API_URL=https://your-backend-url.com

REACT_APP_SUPABASE_URL=your REACT_APP_SUPABASE_URL 
REACT_APP_SUPABASE_ANON_KEY=your key here 
REACT_APP_API_URL=your react api url here 